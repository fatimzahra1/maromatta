{
        "env":  {
                "browser":  true,
                "es2021":  true
        },
        "extends":  [
                "plugin:react/recommended",
"plugin:react/jsx-runtime"
        ],
        "parser":  "babel-eslint",
        "parserOptions":  {
                "ecmaFeatures":  {
                        "jsx":  true
                },
                "ecmaVersion":  12,
                "sourceType":  "module"
        },
        "plugins":  ["jsx-a11y",
                "react"
        ],
        "rules":  {
                "no-multiple-empty-lines":  [
                        "error",
                        {
                                "max":  2,
                                "maxEOF":  1
                        }
                ],
                "comma-dangle":  [
                        "error",
                        {
                                "arrays":  "never",
                                "objects":  "never",
                                "imports":  "never",
                                "exports":  "never",
                                "functions":  "ignore"
                        }
                ],
                "no-console":  0
        },
        "settings":  {

"react":  {
            "version":  "detect"
        },
                "import/resolver":  {
                        "node":  {
                                "paths":  [
                                        "src"
                                ],
                                "extensions":  [
                                        ".js",
                                        ".jsx",
                                        ".ts",
                                        ".tsx"
                                ]
                        }
                }
        }

}